import java.util.HashMap;
import java.util.Map;

public class parsear_String {

	public static void main(String[] args) {
		String text = "hla que tal, bienvenidos a BettaTech. SI os está gustando este vídeo y darle a la campanita para ver los nuevos vídeos que vaya subiendo!";

	}

	String wordRepetitions(String text) {

		Map<String, Integer> map = new HashMap<String, Integer>();

		String separateWords[] = text.split(" ");

		for (String word : separateWords) {
			if (map.containsKey(word)) {
				int aux = map.get(word);
				aux++;
				map.put(word, aux);
			} else {
				map.put(word, 1);
			}
		}
		return "d";
	}

}
